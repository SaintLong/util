<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorF4"
    android:orientation="vertical">

    <com.saint.widget.MyActionBar
        android:id="@+id/myActionBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.google.android.material.imageview.ShapeableImageView
        android:layout_width="@dimen/dp_80"
        android:layout_height="@dimen/dp_80"
        android:layout_margin="@dimen/dp_20"
        android:background="@color/white"
        android:padding="@dimen/dp_2"
        android:src="@mipmap/logo"
        app:shapeAppearance="@style/RoundStyle"
        app:strokeColor="@color/redC9"
        app:strokeWidth="@dimen/dp_0" />

    <com.google.android.material.imageview.ShapeableImageView
        android:layout_width="@dimen/dp_80"
        android:layout_height="@dimen/dp_80"
        android:layout_margin="@dimen/dp_5"
        android:background="@color/white"
        android:padding="@dimen/dp_2"
        android:src="@mipmap/logo"
        app:shapeAppearance="@style/CircleStyle"
        app:strokeColor="@color/redC9"
        app:strokeWidth="@dimen/dp_0" />

    <com.google.android.material.imageview.ShapeableImageView
        android:layout_width="@dimen/dp_80"
        android:layout_height="@dimen/dp_80"
        android:layout_margin="@dimen/dp_5"
        android:background="@color/white"
        android:padding="@dimen/dp_2"
        android:src="@mipmap/logo"
        app:shapeAppearance="@style/CutStyle"
        app:strokeColor="@color/redC9"
        app:strokeWidth="@dimen/dp_0" />

    <com.google.android.material.imageview.ShapeableImageView
        android:layout_width="@dimen/dp_80"
        android:layout_height="@dimen/dp_80"
        android:layout_margin="@dimen/dp_5"
        android:background="@color/white"
        android:padding="@dimen/dp_2"
        android:src="@mipmap/logo"
        app:shapeAppearance="@style/RhombusStyle"
        app:strokeColor="@color/redC9"
        app:strokeWidth="@dimen/dp_0" />

    <com.google.android.material.imageview.ShapeableImageView
        android:layout_width="@dimen/dp_80"
        android:layout_height="@dimen/dp_80"
        android:layout_margin="@dimen/dp_5"
        android:background="@color/white"
        android:padding="@dimen/dp_2"
        android:src="@mipmap/logo"
        app:shapeAppearance="@style/LeafStyle"
        app:strokeColor="@color/redC9"
        app:strokeWidth="@dimen/dp_0" />

    <com.google.android.material.imageview.ShapeableImageView
        android:layout_width="@dimen/dp_140"
        android:layout_height="@dimen/dp_80"
        android:layout_margin="@dimen/dp_5"
        android:background="@color/white"
        android:padding="@dimen/dp_2"
        android:src="@mipmap/logo"
        app:shapeAppearance="@style/HexagonStyle"
        app:strokeColor="@color/redC9"
        app:strokeWidth="@dimen/dp_0" />


</androidx.appcompat.widget.LinearLayoutCompat>